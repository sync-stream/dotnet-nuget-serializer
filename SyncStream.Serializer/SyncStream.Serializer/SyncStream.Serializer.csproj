<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Authors>syncstream-devops</Authors>
        <Company>SyncStream Solutions</Company>
        <Description>
            This library contains the serializers for the SyncStream system
        </Description>
        <DocumentationFile>https://github.com/sync-stream/dotnet-nuget-serializer</DocumentationFile>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <PackageId>SyncStream.Serializer</PackageId>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
        <RepositoryUrl>https://github.com/sync-stream/dotnet-nuget-serializer</RepositoryUrl>
        <RootNamespace>SyncStream.Serializer</RootNamespace>
        <TargetFramework>net6.0</TargetFramework>
        <Version>1.0.3</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MediaTypeMap" Version="2.1.0" />
    </ItemGroup>

</Project>
